{
  "name": "telemetryProject",
  "version": "1.0.50",
  "type": "module",
  "main": "dist/index.js",
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node",
    "build:tsc": "tsc",
    "start": "bun run src/index.ts",
    "start:node": "node dist/index.js",
    "dev": "bun run --watch src/index.ts",
    "vercel-build": "bun run build:tsc",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:generate": "drizzle-kit generate --config=drizzle/drizzle.config.ts",
    "db:migrate": "drizzle-kit migrate --config=drizzle/drizzle.config.ts",
    "db:push": "drizzle-kit push --config=drizzle/drizzle.config.ts",
    "db:studio": "drizzle-kit studio --config=drizzle/drizzle.config.ts"
  },
  "dependencies": {
    "@elysiajs/opentelemetry": "^1.4.0",
    "@libsql/client": "^0.15.15",
    "@opentelemetry/api": "^1.9.0",
    "drizzle-orm": "^0.44.5",
    "elysia": "latest",
    "prom-client": "^15.1.3",
    "sqlite": "^5.1.1",
    "@types/node": "^20.0.0",
    "@vercel/node": "^3.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "bun-types": "latest",
    "drizzle-kit": "^0.31.4"
  },
  "trustedDependencies": ["elysia"],
  "engines": {
    "node": ">=18.0.0"
  }
}
